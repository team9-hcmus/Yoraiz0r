void CountPositive(int a[][100], int m, int n)
{
	int j,i,count = 0;
	for (i = 0; i < m; i++)
	{
		for (j = 0; j < n; j++) 
		{
			if (i == 0 || j == 0 || i == n - 1 || j == n - 1)
				cout << a[i][j] << " ";
			else
				cout << " "
				<< " ";
		}
		cout << "\n";
	}

	for (i = 0; i<m; i++)
	for (j = 0; j<n; j++)
	{
		if (a[i][j]>0)
			count++;
	}
	printf("Co %d so duong o ria Ma Tran tren", count);
}
